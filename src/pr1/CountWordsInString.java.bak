class CountWordsInString {
	public static void main(String [] args){
		String str1="Hello I am Rajendra";
		//String []str=str1.split(" ");
		int count=0;
		char ch[]=str1.toCharArray();

		String s="";
		for (int i=0;i<ch.length ;i++ )
		{
			if (ch[i]==' ')
			{	
				count++;
				s+=ch[i];
			}
			System.out.println(count);
		}
		
	}
}